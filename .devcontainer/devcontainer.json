{
  "name": "Amazon Q DevContainer",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--env-file", ".env"
  ],
  "remoteUser": "developer",
  "mounts": [
    "source=${localEnv:HOME}/.aws,target=/home/developer/.aws,type=bind,consistency=cached"
  ],
  "workspaceMount": "source=/Users/takeya_ozawa/works/poc-something/auto-fill-tool,target=/workspace,type=bind,consistency=cached",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "amazonwebservices.aws-toolkit-vscode",
        "ms-vscode.vscode-json",
        "rust-lang.rust-analyzer",
        "vadimcn.vscode-lldb",
        "ms-vscode.test-adapter-converter"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "rust-analyzer.cargo.buildScripts.enable": true,
        "aws.telemetry": false
      }
    }
  },
  "forwardPorts": [],
  "postCreateCommand": "/usr/local/bin/setup-proxy.sh && /usr/local/bin/setup-aws-cli.sh && /usr/local/bin/setup-certificates.sh && sudo /usr/local/bin/build-amazon-q.sh",
  "features": {}
}
