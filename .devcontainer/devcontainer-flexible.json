{
  "name": "Amazon Q DevContainer (Flexible)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--env-file", ".env"
  ],
  "remoteUser": "developer",
  "mounts": [
    "source=${localEnv:HOME}/.aws,target=/home/developer/.aws,type=bind,consistency=cached",
    "source=${localWorkspaceFolder}/../..,target=/workspaces,type=bind,consistency=cached"
  ],
  "workspaceFolder": "/workspaces",
  "customizations": {
    "vscode": {
      "extensions": [
        "amazonwebservices.aws-toolkit-vscode",
        "ms-vscode.vscode-json",
        "rust-lang.rust-analyzer",
        "vadimcn.vscode-lldb",
        "ms-vscode.test-adapter-converter"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "rust-analyzer.cargo.buildScripts.enable": true,
        "aws.telemetry": false
      }
    }
  },
  "forwardPorts": [],
  "postCreateCommand": "./.devcontainer/scripts/startup-log.sh",
  "features": {}
}
